{
  "name": "course-search",
  "version": "0.1.0",
  "productName": "Course Search",
  "author": "Burhanuddin Udaipurwala <burhanuday>",
  "main": "electron/main.js",
  "private": true,
  "dependencies": {
    "antd": "^3.19.8",
    "axios": "^0.19.0",
    "he": "^1.2.0",
    "lodash.debounce": "^4.0.8",
    "open": "^6.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.3"
  },
  "homepage": "./",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "react-scripts start",
    "start-electron": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "clean": "shx rm -rf build dist",
    "prepackage": "react-scripts build",
    "package": "shx mkdir build/src && shx cp -r electron/. build/electron && shx cp -r src/shared/. build/src/shared",
    "postpackage": "build --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.burhanuday.courseSearch",
    "productName": "Course Search",
    "copyright": "Copyright Â© 2019 burhanuday",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "course-search",
      "owner": "burhanuday"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cross-env": "^5.2.0",
    "electron": "^7.2.4",
    "electron-builder": "^20.44.4",
    "shx": "^0.3.2"
  }
}
